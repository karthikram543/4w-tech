public class musk {
	
		 public static void main (String[]args) {
	
			 String str="this is a string";
			 String str1=str.toLowerCase();
			 char[]arr=str1.toCharArray();
			 
			 for(int i=0;i<arr.length;i++) {
				 int count=0;
				 for(int j=i+1;j<arr.length;j++) {
					 
					 if(arr[i]==arr[j]) {
						 count++;
						 arr[j]='1';
						 
					 }
					 
				 }
				 if(count<=0 && arr[i]!='1') {
					 System.out.println(arr[i]);
				 }
			 }
}}

output:

h
a
r
n
g
