public class musk {
	
	    public static void main(String[] args) {
	        
	    	int arr[]= {1,2,5};
	    	int arr1[]= {3,6,7};
	    	int temp[]=new int[arr.length];
	    	
	    	for(int i=0;i<arr.length;i++) {
	    		temp[i]=arr[i];
	    		arr[i]=arr1[i];
	    		arr1[i]=temp[i];
	    	}
	    	
	    	System.out.println(Arrays.toString(arr));
	    	System.out.println(Arrays.toString(arr1));
	    	
	    	
	    }
	}

output:

[3, 6, 7]
[1, 2, 5]
